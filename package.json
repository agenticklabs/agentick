{
  "name": "agentick",
  "version": "0.0.1",
  "private": true,
  "description": "The component framework for AI.",
  "keywords": [
    "agent",
    "ai",
    "framework",
    "harness",
    "jsx"
  ],
  "homepage": "https://github.com/agenticklabs/agentick#readme",
  "bugs": {
    "url": "https://github.com/agenticklabs/agentick/issues"
  },
  "license": "MIT",
  "author": "Ryan Lindgren",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agenticklabs/agentick.git"
  },
  "type": "module",
  "scripts": {
    "prepare": "simple-git-hooks",
    "build": "turbo build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint:all": "turbo lint && eslint .",
    "lint:fix": "eslint . --fix",
    "format": "oxfmt --write .",
    "format:check": "turbo format:check",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "publish-packages": "turbo run test build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.0.0",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@types/node": "^25.0.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-plugin-oxlint": "^1.43.0",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "simple-git-hooks": "^2.13.1",
    "turbo": "^2.8.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "zod": "^4.1.13"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm run format:check && pnpm run lint:all",
    "commit-msg": "npx --no -- commitlint --config commitlint.config.cjs --edit ${1}"
  },
  "packageManager": "pnpm@10.25.0"
}
