{
  "compilerOptions": {
    "target": "ES2022",
    "module": "preserve",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "jsxImportSource": "@agentick/core",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "paths": {
      "@agentick/core": ["../packages/core/src/index.ts"],
      "@agentick/core/*": ["../packages/core/src/*/index.ts"],
      "@agentick/kernel": ["../packages/kernel/src/index.ts"],
      "@agentick/kernel/*": ["../packages/kernel/src/*/index.ts"],
      "@agentick/shared": ["../packages/shared/src/index.ts"],
      "@agentick/shared/*": ["../packages/shared/src/*/index.ts"],
      "@agentick/gateway": ["../packages/gateway/src/index.ts"],
      "@agentick/client": ["../packages/client/src/index.ts"],
      "@agentick/server": ["../packages/server/src/index.ts"],
      "@agentick/express": ["../packages/express/src/index.ts"],
      "@agentick/react": ["../packages/react/src/index.ts"],
      "@agentick/devtools": ["../packages/devtools/src/index.ts"],
      "@agentick/agent": ["../packages/agent/src/index.ts"],
      "@agentick/guardrails": ["../packages/guardrails/src/index.ts"],
      "@agentick/openai": ["../packages/adapters/openai/src/index.ts"],
      "@agentick/google": ["../packages/adapters/google/src/index.ts"],
      "@agentick/ai-sdk": ["../packages/adapters/ai-sdk/src/index.ts"]
    }
  },
  "include": [
    "../packages/core/src/index.ts",
    "../packages/core/src/app/index.ts",
    "../packages/core/src/hooks/index.ts",
    "../packages/core/src/jsx/index.ts",
    "../packages/core/src/compiler/index.ts",
    "../packages/core/src/tool/index.ts",
    "../packages/core/src/model/index.ts",
    "../packages/core/src/com/index.ts",
    "../packages/core/src/testing/index.ts",
    "../packages/kernel/src/index.ts",
    "../packages/shared/src/index.ts",
    "../packages/shared/src/blocks/index.ts",
    "../packages/shared/src/tools/index.ts",
    "../packages/shared/src/input/index.ts",
    "../packages/shared/src/messages/index.ts",
    "../packages/shared/src/streaming/index.ts",
    "../packages/shared/src/models/index.ts",
    "../packages/shared/src/timeline/index.ts",
    "../packages/shared/src/errors/index.ts",
    "../packages/gateway/src/index.ts",
    "../packages/client/src/index.ts",
    "../packages/server/src/index.ts",
    "../packages/express/src/index.ts",
    "../packages/react/src/index.ts",
    "../packages/devtools/src/index.ts",
    "../packages/agent/src/index.ts",
    "../packages/guardrails/src/index.ts",
    "../packages/adapters/openai/src/index.ts",
    "../packages/adapters/google/src/index.ts",
    "../packages/adapters/ai-sdk/src/index.ts"
  ],
  "exclude": ["**/node_modules/**", "**/*.spec.ts", "**/*.test.ts", "**/examples/**"]
}
