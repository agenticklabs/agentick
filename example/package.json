{
  "name": "tentickle-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently -n express,react -c blue,green \"pnpm --filter example-express dev\" \"pnpm --filter example-react dev\"",
    "dev:express": "pnpm --filter example-express dev",
    "dev:gateway": "pnpm --filter example-express gateway",
    "dev:react": "pnpm --filter example-react dev",
    "dev:angular": "pnpm --filter example-angular dev",
    "build": "turbo run build",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "turbo": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
