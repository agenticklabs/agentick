{
  "name": "@agentick/angular",
  "version": "0.5.0",
  "description": "Angular services and utilities for Agentick applications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agenticklabs/agentick.git",
    "directory": "packages/angular"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@agentick/client": "workspace:*",
    "@agentick/shared": "workspace:*"
  },
  "devDependencies": {
    "@angular/core": "^19.0.0",
    "@types/node": "^22.10.5",
    "rxjs": "^7.8.0",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "@angular/core": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "rxjs": "^7.0.0"
  }
}
