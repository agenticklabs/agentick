{
  "name": "@tentickle/angular",
  "version": "0.0.1",
  "description": "Angular services and utilities for Tentickle applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "@angular/core": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@angular/core": "^19.0.0",
    "@types/node": "^22.10.5",
    "rxjs": "^7.8.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@tentickle/client": "workspace:*",
    "@tentickle/shared": "workspace:*"
  }
}
