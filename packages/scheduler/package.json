{
  "name": "@agentick/scheduler",
  "version": "0.7.0",
  "description": "Scheduled jobs, heartbeat, and cron triggers for Agentick agents",
  "keywords": [
    "agent",
    "ai",
    "cron",
    "heartbeat",
    "scheduler"
  ],
  "license": "MIT",
  "author": "Ryan Lindgren",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agenticklabs/agentick.git",
    "directory": "packages/scheduler"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "echo \"Tests run from workspace root\"",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "lint": "oxlint src/",
    "format:check": "oxfmt --check src/",
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo",
    "prepublishOnly": "pnpm build",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@agentick/client": "workspace:*",
    "@agentick/core": "workspace:*",
    "@agentick/shared": "workspace:*",
    "node-cron": "^3.0.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}
