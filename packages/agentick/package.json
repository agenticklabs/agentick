{
  "name": "agentick",
  "version": "0.9.2",
  "description": "The component framework for AI.",
  "keywords": [
    "agent",
    "ai",
    "framework",
    "jsx",
    "react"
  ],
  "license": "MIT",
  "author": "Ryan Lindgren",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agenticklabs/agentick.git",
    "directory": "packages/agentick"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./jsx-runtime": "./src/jsx-runtime.ts",
    "./jsx-dev-runtime": "./src/jsx-runtime.ts"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./jsx-runtime": {
        "types": "./dist/jsx-runtime.d.ts",
        "import": "./dist/jsx-runtime.js"
      },
      "./jsx-dev-runtime": {
        "types": "./dist/jsx-runtime.d.ts",
        "import": "./dist/jsx-runtime.js"
      }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo",
    "prepublishOnly": "cp ../../README.md . && pnpm build"
  },
  "dependencies": {
    "@agentick/agent": "workspace:*",
    "@agentick/core": "workspace:*",
    "@agentick/guardrails": "workspace:*"
  }
}
