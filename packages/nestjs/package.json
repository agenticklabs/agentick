{
  "name": "@tentickle/nestjs",
  "version": "0.0.1",
  "description": "NestJS module for Tentickle servers",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@tentickle/server": "workspace:*",
    "@tentickle/shared": "workspace:*"
  },
  "peerDependencies": {
    "@nestjs/common": ">=9.0.0",
    "@nestjs/core": ">=9.0.0",
    "reflect-metadata": ">=0.1.0",
    "rxjs": ">=7.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/testing": "^10.4.15",
    "@types/express": "^4.17.25",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "tentickle",
    "nestjs",
    "ai",
    "llm",
    "agent"
  ],
  "license": "MIT"
}
