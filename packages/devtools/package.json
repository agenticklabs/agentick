{
  "name": "@tentickle/devtools",
  "version": "0.0.1",
  "files": [
    "dist",
    "ui/dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && pnpm build:ui",
