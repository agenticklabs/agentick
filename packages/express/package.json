{
  "name": "@agentick/express",
  "version": "0.0.1",
  "description": "Express middleware and router for Agentick applications",
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@agentick/gateway": "workspace:*"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.5",
    "@types/supertest": "^6.0.2",
    "express": "^4.21.0",
    "supertest": "^7.0.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0"
  }
}
