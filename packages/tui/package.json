{
  "name": "@agentick/tui",
  "version": "0.6.0",
  "description": "Terminal UI for Agentick agents using Ink",
  "keywords": [
    "agent",
    "ai",
    "ink",
    "terminal",
    "tui"
  ],
  "license": "MIT",
  "author": "Ryan Lindgren",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agenticklabs/agentick.git",
    "directory": "packages/tui"
  },
  "bin": {
    "agentick-tui": "./dist/bin.js"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo",
    "lint": "oxlint src/",
    "format:check": "oxfmt --check src/",
    "start": "tsx --tsconfig tsconfig.json src/bin.ts",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts"
  },
  "dependencies": {
    "@agentick/client": "workspace:*",
    "@agentick/core": "workspace:*",
    "@agentick/react": "workspace:*",
    "@agentick/shared": "workspace:*",
    "chalk": "^5.6.2",
    "eventsource": "^2.0.2",
    "ink": "^5.1.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.2",
    "@types/eventsource": "^1.1.15",
    "@types/marked-terminal": "^6.1.1",
    "@types/react": "^19.2.13",
    "ink-testing-library": "^4.0.0",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  }
}
