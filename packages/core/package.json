{
  "name": "@agentick/core",
  "version": "0.7.0",
  "description": "Core engine for Agentick",
  "keywords": [
    "agent",
    "ai",
    "core"
  ],
  "license": "MIT",
  "author": "Ryan Lindgren",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agenticklabs/agentick.git",
    "directory": "packages/core"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./src/jsx/jsx-runtime.ts",
      "import": "./dist/jsx/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./src/jsx/jsx-runtime.ts",
      "import": "./dist/jsx/jsx-runtime.js"
    },
    "./app": {
      "types": "./src/app.ts",
      "import": "./dist/app.js"
    },
    "./model": {
      "types": "./src/model/index.ts",
      "import": "./dist/model/index.js"
    },
    "./com": {
      "types": "./src/com/index.ts",
      "import": "./dist/com/index.js"
    },
    "./jsx": {
      "types": "./src/jsx/index.ts",
      "import": "./dist/jsx/index.js"
    },
    "./hooks": {
      "types": "./src/compiler/index.ts",
      "import": "./dist/compiler/index.js"
    },
    "./compiler": {
      "types": "./src/compiler/index.ts",
      "import": "./dist/compiler/index.js"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "import": "./dist/utils/index.js"
    },
    "./tool": {
      "types": "./src/tool/index.ts",
      "import": "./dist/tool/index.js"
    },
    "./model/utils/language-model": {
      "types": "./src/model/utils/language-model.ts",
      "import": "./dist/model/utils/language-model.js"
    },
    "./testing": {
      "types": "./src/testing/index.ts",
      "import": "./dist/testing/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "echo \"Tests run from workspace root\"",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "lint": "oxlint src/",
    "format:check": "oxfmt --check src/",
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo",
    "prepublishOnly": "pnpm build",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@agentick/kernel": "workspace:*",
    "@agentick/shared": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "react": "^19.2.4",
    "react-reconciler": "^0.33.0",
    "zod": "^4.3.6",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@types/react": "^19.2.13",
    "@types/react-reconciler": "0.33.0"
  }
}
