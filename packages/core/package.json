{
  "name": "@tentickle/core",
  "version": "0.0.1",
  "description": "Core engine for Tentickle",
  "keywords": [
    "agent",
    "ai",
    "core"
  ],
  "license": "ISC",
  "author": "Ryan Lindgren",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./src/core/index.ts",
      "import": "./dist/core/index.js"
    },
    "./jsx-runtime": {
      "types": "./src/jsx/jsx-runtime.ts",
      "import": "./dist/jsx/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./src/jsx/jsx-runtime.ts",
      "import": "./dist/jsx/jsx-runtime.js"
    },
    "./app": {
      "types": "./src/app.ts",
      "import": "./dist/app.js"
    },
    "./model": {
      "types": "./src/model/index.ts",
      "import": "./dist/model/index.js"
    },
    "./com": {
      "types": "./src/com/index.ts",
      "import": "./dist/com/index.js"
    },
    "./jsx": {
      "types": "./src/jsx/index.ts",
      "import": "./dist/jsx/index.js"
    },
    "./hooks": {
      "types": "./src/compiler/index.ts",
      "import": "./dist/compiler/index.js"
    },
    "./compiler": {
      "types": "./src/compiler/index.ts",
      "import": "./dist/compiler/index.js"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "import": "./dist/utils/index.js"
    },
    "./tool": {
      "types": "./src/tool/index.ts",
      "import": "./dist/tool/index.js"
    },
    "./model/utils/language-model": {
      "types": "./src/model/utils/language-model.ts",
      "import": "./dist/model/utils/language-model.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./core": {
        "types": "./dist/core/index.d.ts",
        "import": "./dist/core/index.js"
      }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "echo \"Tests run from workspace root\"",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "lint": "oxlint src/",
    "format:check": "oxfmt --check src/",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@tentickle/shared": "workspace:*",
    "pino": "^9.6.0",
    "zod": "^4.2.1",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "pino-pretty": "^13.0.0"
  }
}
