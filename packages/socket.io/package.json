{
  "name": "@tentickle/socket.io",
  "version": "0.0.1",
  "description": "Socket.IO transport for Tentickle client and server",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "socket.io": "^4.0.0",
    "socket.io-client": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "socket.io": {
      "optional": true
    },
    "socket.io-client": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@tentickle/shared": "workspace:*"
  }
}
